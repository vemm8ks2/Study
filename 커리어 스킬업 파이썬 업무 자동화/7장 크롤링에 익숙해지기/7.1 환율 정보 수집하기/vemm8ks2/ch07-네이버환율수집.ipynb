    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "\n",
    "code = requests.get('https://finance.naver.com/marketindex/')\n",
    "html = BeautifulSoup(code.text, 'html.parser')\n",
    "\n",
    "price = html.select('ul#exchangeList span.value')\n",
    "\n",
    "for i in price:\n",
    "    print(i.text)"
